<body>
  <!-- <div class="container"> -->
    <% if current_user %>
    <div class="navbar-wagon">
      <!-- Logo -->
      <a href="/" class="navbar-wagon-brand">
        <%= image_tag "rent.png" %>
      </a>

      <!-- Right Navigation -->
      <div class="navbar-wagon-right hidden-xs hidden-sm">

        <%= link_to "Home", users_path, class:"navbar-wagon-item navbar-wagon-link"%>
        <!-- <a href="" class="navbar-wagon-item navbar-wagon-link">Home</a> -->
        <a href="" class="navbar-wagon-item navbar-wagon-link">Hi, <%= current_user.name %></a>

        <!-- Profile picture with dropdown list -->
        <div class="navbar-wagon-item">
          <div class="dropdown">

            <%= image_tag "plate.png", class: "avatar dropdown-toggle", id:"navbar-wagon-menu", "data-toggle":"dropdown"%>

           <!--  <img src="https://simg.nicepng.com/png/small/16-168788_clipart-freeuse-stock-chef-hat-clipart-chef-hat.png" class="avatar dropdown-toggle" id="navbar-wagon-menu" data-toggle="dropdown"> -->

            <ul class="dropdown-menu dropdown-menu-right navbar-wagon-dropdown-menu">
              <li><%= link_to "Dashboard", bookings_path, class:"navbar-wagon-item navbar-wagon-link"%></li>
              <li><%= link_to "Register to be a chef", edit_user_path(current_user.id), class:"navbar-wagon-item navbar-wagon-link"%></li>
              <li><%= link_to "Log out", destroy_user_session_path, method: :delete, class:"navbar-wagon-item navbar-wagon-link"%></li>
            </ul>
          </div>
        </div>
      </div>

      <!-- Dropdown appearing on mobile only -->
      <div class="navbar-wagon-item hidden-md hidden-lg">
        <div class="dropdown">
          <i class="fas fa-bars dropdown-toggle" data-toggle="dropdown" role="button"></i>
          <ul class="dropdown-menu dropdown-menu-right navbar-wagon-dropdown-menu">
            <li><a href="#">Host</a></li>
            <li><a href="#">Trips</a></li>
            <li><a href="#">Messagese</a></li>
          </ul>
        </div>
      </div>
    </div>
  <% else %>

  <div class="navbar-wagon">
      <!-- Logo -->
      <a href="/" class="navbar-wagon-brand">
        <%= image_tag "rent.png" %>
      </a>

      <!-- Right Navigation -->
      <div class="navbar-wagon-right hidden-xs hidden-sm">

        <%= link_to "Login", new_user_session_path, class:"navbar-wagon-item navbar-wagon-link"%>
        <%= link_to "Home", root_path, class:"navbar-wagon-item navbar-wagon-link"%>
        <!-- <a href="" class="navbar-wagon-item navbar-wagon-link">Home</a> -->


        <!-- Profile picture with dropdown list -->
      </div>

      <!-- Dropdown appearing on mobile only -->
      <div class="navbar-wagon-item hidden-md hidden-lg">
        <div class="dropdown">
          <i class="fas fa-bars dropdown-toggle" data-toggle="dropdown" role="button"></i>
          <ul class="dropdown-menu dropdown-menu-right navbar-wagon-dropdown-menu">
            <li><a href="#">Host</a></li>
            <li><a href="#">Trips</a></li>
            <li><a href="#">Messagese</a></li>
          </ul>
        </div>
      </div>
    </div>


  <% end %>
